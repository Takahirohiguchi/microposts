<h1>User Profile Edit</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      
      名前  ：  
      <%= f.text_field :name %>
      
      Email  ：  
      <%= f.text_field :email %>

      Profile  :  
      <%= f.text_field :profile %>

      地域  ：  
      <%= f.text_field :area %>
 
      <%= f.submit "edit profile", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>